  <analysisEngineMetaData>
    <name>openease</name>
    <description>pipeline for openEase</description>
    <version>1.0</version>
    <vendor/>

      <configurationParameters searchStrategy="none">

      <configurationParameter>
        <name>camera_config_files</name>
        <description>path to config file that holds info about camera topics and tf frame names</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>CollectionReader/camera_config_files</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>wordnetGrounded</name>
        <description></description>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>MLNAtomsGenerator/wordnetGrounded</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>query_mln</name>
        <description></description>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>MLNAtomsGenerator/query_mln</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>clearStorageOnStart</name>
        <description/>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>StorageWriter/clearStorageOnStart</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>filter_results</name>
        <description>Filter the results based on the request designator</description>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ShoppingResultAdvertiser/filter_results</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>resultMode</name>
        <description>Use "Cluster" or "Object".</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ShoppingResultAdvertiser/resultMode</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>storagedb</name>
        <description/>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>StorageWriter/db</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>hsvFilter</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ClusterMerger/hsvFilter</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>cluster_min_size</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>PointCloudClusterExtractor/cluster_min_size</parameter>
        </overrides>
      </configurationParameter>

    </configurationParameters>

    <configurationParameterSettings>
      <nameValuePair>
        <name>wordnetGrounded</name>
        <value>        
            <boolean>false</boolean>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>query_mln</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>filter_results</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>resultMode</name>
        <value>
          <string>Cluster</string>
        </value>
      </nameValuePair>

       <nameValuePair>
        <name>camera_config_files</name>
        <value>
          <array>
               <string>config_mongodb_openease.ini</string>
<!--              <string>config_Kinect_robot.ini</string>-->
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>clearStorageOnStart</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>storagedb</name>
        <value>
          <string>Scenes_annotated</string>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>hsvFilter</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>cluster_min_size</name>
        <value>
          <integer>500</integer>
        </value>
      </nameValuePair>

    </configurationParameterSettings>
    <flowConstraints>
        <fixedFlow>
      <node>Trigger</node>
      <node>CollectionReader</node>
      <node>ImagePreprocessor</node>
      <node>HandleAnnotator</node>
      <node>RegionFilter</node>
