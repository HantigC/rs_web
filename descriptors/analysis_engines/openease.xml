      <node>ClusterColorHistogramCalculator</node>
      <node>PrimitiveShapeAnnotator</node>
      <node>SacModelAnnotator</node>
      <node>LinemodAnnotator</node>
      <node>MLNInferencer</node>
      <node>PancakeAnnotator</node>
<node>ClusterToPartsSegmenter</node>
<!--      <node>ClusterTracker</node>-->
<!--      <node>ClusterGogglesAnnotator</node>-->
<!--      <node>GroundTruthAnnotator</node>-->
<!--      <node>MLNAtomsGenerator</node>-->
      <node>StorageWriter</node>
      <node>KBResultAdvertiser</node>
<!--      <node>VisualizerAnnotator</node>-->
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection>
      <fsIndexes>
        <fsIndexDescription>
          <label>general</label>
          <typeName>uima.cas.TOP</typeName>
          <kind>bag</kind>
        </fsIndexDescription>
      </fsIndexes>
    </fsIndexCollection>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported>
